using System;
public class Program{
  public static void Main(string[] args){
    int cont = 1;
    int contb = 1;
    while(cont <= 10){
      Console.WriteLine($"Tabuada de {cont}");
      while(contb <=10){
        Console.WriteLine($"{cont} x {contb} = {cont * contb}");
        contb++;
      }
      Console.WriteLine();
      contb = 1;
      cont++;
    }
  }
}
